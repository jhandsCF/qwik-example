{
  "name": "qwik-app",
  "description": "Starter with built-in routing, powered by Qwik City Vite.js tooling. Cloudflare Pages. Eslint code linting. Prettier code formatter. Tailwind CSS framework.",
  "scripts": {
    "build": "npm run typecheck && npm run build.client && npm run build.ssr",
    "build.client": "vite build",
    "build.ssr": "vite build --ssr src/entry.cloudflare.tsx",
    "dev": "vite",
    "dev.debug": "node --inspect-brk node_modules/vite/bin/vite.js",
    "dev.ssr": "vite --mode ssr",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"**/*.ts*\"",
    "serve": "wrangler pages dev ./dist",
    "start": "npm run dev",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.0.21",
    "@builder.io/qwik-city": "0.0.10",
    "@cloudflare/workers-types": "3.9.0",
    "@types/eslint": "8.4.2",
    "@types/node": "17.0.34",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "autoprefixer": "10.4.7",
    "eslint": "8.15.0",
    "eslint-plugin-qwik": "latest",
    "node-fetch": "2.6.7",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.4",
    "vite": "2.9.9",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://qwik.builder.io/",
  "license": "",
  "private": true
}
